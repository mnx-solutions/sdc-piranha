{
  "server": {
      "port": 3000
  },
  "log": {
      "name": "piranha",
      "level": "debug"
  },
	"cloudapi": {
	},
  "modulizer": {
      "minify": true,
      "assetize": true
  },
	"static":{
		"js": {
			"concat": ["use-strict-rm"],
			"cache": "all",
			"ext": "js",
			"transform": ["use-strict-add","uglify-js","gzip"]
		},
		"css": {
			"concat":true,
			"cache": "all",
			"ext": "css",
			"transform": ["less", "sqwish", "gzip"]
		}
	}
}